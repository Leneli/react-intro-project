{
	// See https://go.microsoft.com/fwlink/?LinkId=733558
	// for the documentation about the tasks.json format
	"version": "2.0.0",
	"tasks": [
		{
			"label": "development",
			"type": "shell",
			"command": "yarn start",
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"problemMatcher": [
				"$go"
			]
		},
		{
			"label": "production",
			"type": "shell",
			"command": "yarn build",
			"problemMatcher": [
				"$go"
			]
		},
		{
			"label": "test",
			"type": "shell",
			"command": "yarn test",
			"problemMatcher": [
				"$go"
			]
		},
		{
			"label": "eject",
			"type": "shell",
			"command": "yarn eject",
			"problemMatcher": [
				"$go"
			]
		}
	]
}
